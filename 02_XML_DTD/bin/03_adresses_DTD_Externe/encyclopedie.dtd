<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT encyclopedie     (personne*)>
<!ELEMENT personne  (nom,prenom,publication+,img?)>
<!ELEMENT nom       (#PCDATA)>
<!ELEMENT prenom    (#PCDATA)>
<!ELEMENT publication (#PCDATA)>
